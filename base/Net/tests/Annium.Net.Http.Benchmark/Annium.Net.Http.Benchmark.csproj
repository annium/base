<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>Annium.Net.Http.Benchmark</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Annium.Net.Http.Benchmark</Description>
        <TargetFramework>net7.0</TargetFramework>
        <OutputType>Exe</OutputType>
        <DebugType>portable</DebugType>
        <LangVersion>11.0</LangVersion>
        <WarningsAsErrors>true</WarningsAsErrors>
        <IsPackable>false</IsPackable>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="../../../Logging/src/Annium.Logging.InMemory/Annium.Logging.InMemory.csproj" />
        <ProjectReference Include="../../src/Annium.Net.Http/Annium.Net.Http.csproj" />
        <ProjectReference Include="../../src/Annium.Net.Servers/Annium.Net.Servers.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="BenchmarkDotNet" Version="0.13.8" />
    </ItemGroup>
</Project>