    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="7.0.0" />
        <PackageReference Include="SimpleBase" Version="4.0.0" />
    </ItemGroup>
</Project>