<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>Annium.Extensions.Execution</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Extension library for simplification of execution flow. Is used for code-level transactions.</Description>
        <TargetFramework>net7.0</TargetFramework>
        <OutputType>Library</OutputType>
        <DebugType>portable</DebugType>
        <LangVersion>11.0</LangVersion>
        <WarningsAsErrors>true</WarningsAsErrors>
        <IsPackable>true</IsPackable>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="../../../Data/src/Annium.Data.Operations/Annium.Data.Operations.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="System.Threading.Channels" Version="7.0.0" />
    </ItemGroup>
</Project>